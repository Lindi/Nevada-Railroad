<nevada>
	<states>
		<state id="attract-loop" initialState="yes">
			<inputs>
				<input type="mouseUp" target="attract-loop" next="screen-20-map-nevada-rails" />
			</inputs>
		</state>
		<state id="title" timeout="5000">
			<inputs>
				<input type="timeout" next="screen-4" />
			</inputs>
		</state>
		<state id="screen-4">
			<inputs>
				<input type="soundComplete" target="sound-screen-04" next="screen-5" />
			</inputs>
		</state>
		<state id="screen-5">
			<inputs>
				<input type="soundComplete" target="sound-screen-05" next="screen-6-map-south-end" />
			</inputs>
		</state>
		<state id="screen-6-map-south-end">
			<inputs>
				<input type="soundComplete" target="sound-screen-06" next="screen-7-map-north-end">
					<transitions>
						<transition name="reset" target="map-south-end">
							<argument type="Boolean" value="true" />
						</transition>
					</transitions>
				</input>
			</inputs>
		</state>
		<state id="screen-7-map-north-end">
			<inputs>
				<input type="complete" target="map-north-end" next="screen-8-map-comparison" />
			</inputs>
		</state>
		<state id="screen-8-map-comparison">
			<transitions>
				<transition name="reset" target="map-comparison">
					<argument type="Boolean" value="true" />
				</transition>
				<transition name="fade" target="map-comparison">
					<argument type="Number" value=".5" />
					<argument type="String" value="north-end" />
				</transition>
			</transitions>
			<inputs>
				<input type="complete" target="map-comparison" next="screen-9-comstock">
					<transitions>
						<transition name="reset" target="map-comparison">
							<argument type="Boolean" value="true" />
						</transition>
					</transitions>
				</input>
			</inputs>
		</state>
		<state id="screen-9-comstock">
			<inputs>
				<input type="soundComplete" target="sound-screen-09" next="screen-10" />
			</inputs>
		</state>
		<state id="screen-10" timeout="200000">
			<inputs>
				<input type="timeout" next="attract-loop" />
				<input type="mouseUp" target="screen-10-view-images" next="screen-11"  />
				<input type="mouseUp" target="screen-10-resume-story" next="screen-13" />
			</inputs>
		</state>
		<state id="screen-11" timeout="200000">
			<inputs>
				<input type="timeout" next="attract-loop" />
				<input type="mouseUp" target="screen-11-resume-story" next="screen-13" />
			</inputs>
		</state>
		<state id="screen-13" timeout="200000">
			<inputs>
				<input type="timeout" next="attract-loop" />
				<input type="mouseUp" target="screen-13-touch-here" next="screen-14-map-pacific" />
			</inputs>
		</state>
		<state id="screen-14-map-pacific">
			<inputs>
				<input type="soundComplete" target="sound-screen-14" next="screen-15-map-pacific">
					<transitions>
						<transition name="track" target="map-pacific">
							<argument type="String" value="union" />
							<argument type="Boolean" value="false" />
						</transition>
						<transition name="setZoom" target="map-pacific">
							<argument type="Number" value="2.125" />
						</transition>
					</transitions>
				</input>
			</inputs>
		</state>
		<state id="screen-15-map-pacific">
			<inputs>
				<input type="soundComplete" target="sound-screen-15" next="screen-16-map-pacific">
					<transitions>
						<transition name="track" target="map-pacific">
							<argument type="String" value="union" />
							<argument type="Boolean" value="true" />
						</transition>
						<transition name="setZoom" target="map-pacific">
							<argument type="Number" value="1" />
						</transition>
					</transitions>
				</input>
			</inputs>
		</state>
		<state id="screen-16-map-pacific">
			<inputs>
				<input type="soundComplete" target="sound-screen-16" next="screen-17">
					<transitions>
						<transition name="reset" target="map-pacific">
							<argument type="Boolean" value="true" />
						</transition>
					</transitions>
				</input>
			</inputs>
		</state>
		<state id="screen-17" timeout="200000">
			<inputs>
				<input type="timeout" next="attract-loop" />
				<input type="mouseUp" target="screen-17-view-images" next="screen-17-gallery"  />
				<input type="mouseUp" target="screen-17-resume-story" next="screen-18" />
			</inputs>
		</state>
		<state id="screen-17-gallery" timeout="200000">
			<inputs>
				<input type="timeout" next="attract-loop" />
				<input type="mouseUp" target="screen-17-gallery-resume-story" next="screen-18" />
			</inputs>
		</state>
		<state id="screen-18" timeout="7000">
			<inputs>
				<input type="timeout" next="screen-19" />
			</inputs>
		</state>
		<state id="screen-19" timeout="2000">
			<inputs>
				<input type="timeout" next="screen-19-buttons" />
			</inputs>
		</state>
		<state id="screen-19-buttons" timeout="200000">
			<inputs>
				<input type="timeout" next="attract-loop" />
				<input type="mouseUp" target="screen-19-view-map" next="screen-20-map-nevada-rails"  />
				<input type="mouseUp" target="screen-19-resume-story" next="screen-22" />
			</inputs>
		</state>
		<state id="screen-20-map-nevada-rails">
			<inputs>
				<input type="complete" target="map-nevada-rails-reverse" next="screen-22">
					<transitions>
						<transition name="reset" target="map-nevada-rails-reverse">
							<argument type="Boolean" value="true" />
						</transition>
						<transition name="reset" target="map-nevada-rails">
							<argument type="Boolean" value="true" />
						</transition>
					</transitions>
				</input>
				<input type="complete" target="map-nevada-rails" next="">
					<transitions>
						<transition type="fade" fade="out" target="label-map-nevada-rails-virginia-truckee" />
						<transition type="fade" fade="out" target="label-map-nevada-rails-carson-colorado" />
						<transition type="fade" fade="out" target="label-map-nevada-rails-central-pacific" />
						<transition type="fade" fade="out" target="label-map-nevada-rails-eureka-palisade" />
						<transition type="fade" fade="out" target="label-map-nevada-rails-nevada-california-oregon" />
						<transition type="fade" fade="out" target="label-map-nevada-rails-nevada-central" />
						<transition type="fade" fade="out" target="label-map-nevada-rails-nevada-northern" />
						<transition type="fade" fade="out" target="label-map-nevada-rails-tonopah-goldfield" />
						<transition name="play" target="sound-screen-21" />
						<transition name="play" target="map-nevada-rails-reverse" />
						<transition name="play" target="map-nevada-mines-reverse" />
						<transition type="fade" fade="out" target="map-nevada-rails" />
						<transition type="fade" fade="out" target="map-nevada-mines" />
					</transitions>
				</input>
				<input type="mouseUp" target="screen-20-resume-story" next="screen-22">
					<transitions>
						<transition name="reset" target="map-nevada-rails">
							<argument type="Boolean" value="true" />
						</transition>
					</transitions>
				</input>
				<input type="mouseUp" target="screen-20-back" next="screen-19-buttons">
					<transitions>
						<transition name="reset" target="map-nevada-rails">
							<argument type="Boolean" value="true" />
						</transition>
					</transitions>
				</input>
				<input type="complete" target="map-nevada-rails" next="">
					<transitions>
						<transition name="play" target="sound-screen-21" />
					</transitions>
				</input>
				<input type="route-start-carson-colorado" target="map-nevada-rails" next="">
					<transitions>
						<transition name="play" target="sound-route-carson-colorado" />
						<transition type="fade" fade="in"  target="label-map-nevada-rails-carson-colorado" />
					</transitions>
				</input>
				<input type="route-start-central-pacific" target="map-nevada-rails" next="">
					<transitions>
						<transition type="fade" fade="in"  target="label-map-nevada-rails-central-pacific" />
						<transition name="play" target="sound-route-central-pacific" />
					</transitions>
				</input>
				<input type="route-start-eureka-palisade" target="map-nevada-rails" next="">
					<transitions>
						<transition type="fade" fade="in"  target="label-map-nevada-rails-eureka-palisade" />
						<transition name="play" target="sound-route-eureka-palisade" />
					</transitions>
				</input>
				<input type="route-start-nevada-california-oregon" target="map-nevada-rails" next="">
					<transitions>
						<transition type="fade" fade="in"  target="label-map-nevada-rails-nevada-california-oregon" />
						<transition name="play" target="sound-route-nevada-california-oregon" />
					</transitions>
				</input>
				<input type="route-start-nevada-central" target="map-nevada-rails" next="">
					<transitions>
						<transition type="fade" fade="in"  target="label-map-nevada-rails-nevada-central" />
						<transition name="play" target="sound-route-nevada-central" />
					</transitions>
				</input>
				<input type="route-start-nevada-northern" target="map-nevada-rails" next="">
					<transitions>
						<transition type="fade" fade="in"  target="label-map-nevada-rails-nevada-northern" />
						<transition name="play" target="sound-route-nevada-northern" />
					</transitions>
				</input>
				<input type="route-start-others" target="map-nevada-rails" next="">
					<transitions>
						<transition name="play" target="sound-route-others" />
						<transition name="play" target="map-nevada-mines" />
					</transitions>
				</input>
				<input type="route-start-tonopah-goldfield" target="map-nevada-rails" next="">
					<transitions>
						<transition type="fade" fade="in"  target="label-map-nevada-rails-tonopah-goldfield" />
						<transition name="play" target="sound-route-tonopah-goldfield" />
					</transitions>
				</input>
				<input type="route-start-virginia-truckee" target="map-nevada-rails" next="">
					<transitions>
						<transition type="fade" fade="in"  target="label-map-nevada-rails-virginia-truckee" />
						<transition name="play" target="sound-route-virginia-truckee" />
					</transitions>
				</input>
			</inputs>
		</state>
		<state id="screen-22" timeout="5000">
			<inputs>
				<input type="timeout" next="screen-23-map-us-all" />
			</inputs>
		</state>
		<state id="screen-23-map-us-all">
			<inputs>
				<input type="complete" target="map-us-all" next="attract-loop">
					<transitions>
						<transition name="reset" target="map-us-all">
							<argument type="Boolean" value="true" />
						</transition>
					</transitions>				
				</input>
			</inputs>
		</state>
	</states>
	<components>
		<component id="attract-loop">
			<component type="loop" id="attract-loop" />
			<component type="image" id="text-1" />
		</component>
		<component id="title">
