			</routes>
			<background>
				<!-- <map id="map-north-end" /> -->
				<image id="us-map" url="/images/map/US-MAP.jpg" />
			</background>	
		</map>
		<!-- The Union and Pacific Rails Map -->
		<map id="map-pacific" width="2336" height="2808" scroll="true">
			<overlays type="trail">
				<overlay x="1036.654" y="2086.807" zoom="2" id="sacramento" color="0xFFEA83">
					<swf id="overlay-sacramento" url="/images/map/overlay-sacramento.swf" x="1049"  y="2059" alpha="0"  />
				</overlay>
				<overlay x="1024.525" y="2070.852" zoom="2" id="newcastle" color="0xFFEA83">
					<swf id="overlay-newcastle" url="/images/map/overlay-newcastle.swf" x="956" y="2043" alpha="0"  />
				</overlay>
				<overlay x="985.351" y="2000.412" zoom="2" id="reno" color="0xFFEA83">
					<swf id="overlay-reno" url="/images/map/overlay-reno.swf" x="917"  y="1972" alpha="0" />
				</overlay>
				<overlay x="986.601" y="1987.329" zoom="2" id="wadsworth" color="0xFFEA83">
					<swf id="overlay-wadsworth" url="/images/map/overlay-wadsworth.swf" x="1000" y="1956" alpha="0"  />
				</overlay>
			</overlays>
			<routes>
				<routes>
					<route id="pacific" url="/map/routes/us/Full-Historic-Route.json" color="0xffffff" thickness="6" arclength=".425" />
					<route id="union" url="/map/routes/us/Full-Historic-Route-Reversed.json" color="0xffffff" thickness="6" arclength="2" />
				</routes>
			</routes>
			<background>
				<image id="us-map" url="/images/map/US-MAP.jpg" />
			</background>	
		</map>
		<!-- The Mines Map -->
		<map id="map-nevada-mines" type="mines" reverse="false" autoPlay="false">
